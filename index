<p class="p1"><span class="Apple-converted-space">    </span>const MONTHS = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function normalizeMinus(s) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>return s.replace(/[\u2212\u2013\u2014]/g, "-");</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p1"><span class="Apple-converted-space">    </span>function parseTemp(val) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (val == null) return NaN;</p>
<p class="p1"><span class="Apple-converted-space">      </span>let s = String(val).trim();</p>
<p class="p1"><span class="Apple-converted-space">      </span>s = normalizeMinus(s);</p>
<p class="p1"><span class="Apple-converted-space">      </span>s = s.replace(/\(.*?\)/g, ""); // drop Fahrenheit</p>
<p class="p1"><span class="Apple-converted-space">      </span>s = s.replace(/\s+/g, "");</p>
<p class="p1"><span class="Apple-converted-space">      </span>s = s.replace(/[^0-9.\-]/g, ""); // digits, dot, minus only</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (!s) return NaN;</p>
<p class="p1"><span class="Apple-converted-space">      </span>return parseFloat(s);</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>// Robust CSV parser for quoted fields</p>
<p class="p1"><span class="Apple-converted-space">    </span>function parseCSV(text) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const rows = [];</p>
<p class="p1"><span class="Apple-converted-space">      </span>let field = "", row = [], inQuotes = false;</p>
<p class="p1"><span class="Apple-converted-space">      </span>for (let i = 0; i &lt; text.length; i++) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const c = text[i];</p>
<p class="p1"><span class="Apple-converted-space">        </span>if (c === '"') {</p>
<p class="p1"><span class="Apple-converted-space">          </span>if (inQuotes &amp;&amp; text[i + 1] === '"') { field += '"'; i++; }</p>
<p class="p1"><span class="Apple-converted-space">          </span>else { inQuotes = !inQuotes; }</p>
<p class="p1"><span class="Apple-converted-space">        </span>} else if ((c === '\n' || c === '\r') &amp;&amp; !inQuotes) {</p>
<p class="p1"><span class="Apple-converted-space">          </span>row.push(field); field = "";</p>
<p class="p1"><span class="Apple-converted-space">          </span>if (row.some(cell =&gt; cell.trim() !== "")) rows.push(row);</p>
<p class="p1"><span class="Apple-converted-space">          </span>row = [];</p>
<p class="p1"><span class="Apple-converted-space">          </span>if (c === '\r' &amp;&amp; text[i + 1] === '\n') i++;</p>
<p class="p1"><span class="Apple-converted-space">        </span>} else if (c === ',' &amp;&amp; !inQuotes) {</p>
<p class="p1"><span class="Apple-converted-space">          </span>row.push(field); field = "";</p>
<p class="p1"><span class="Apple-converted-space">        </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">          </span>field += c;</p>
<p class="p1"><span class="Apple-converted-space">        </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (field.length || row.length) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>row.push(field);</p>
<p class="p1"><span class="Apple-converted-space">        </span>if (row.some(cell =&gt; cell.trim() !== "")) rows.push(row);</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>return rows;</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function textToObjects(text) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const rows = parseCSV(text);</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (!rows.length) return [];</p>
<p class="p1"><span class="Apple-converted-space">      </span>const header = rows[0].map(h =&gt; h.trim());</p>
<p class="p1"><span class="Apple-converted-space">      </span>const dataRows = rows.slice(1);</p>
<p class="p1"><span class="Apple-converted-space">      </span>return dataRows.map(cols =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const obj = {};</p>
<p class="p1"><span class="Apple-converted-space">        </span>for (let i = 0; i &lt; header.length; i++) obj[header[i]] = cols[i] ?? "";</p>
<p class="p1"><span class="Apple-converted-space">        </span>return obj;</p>
<p class="p1"><span class="Apple-converted-space">      </span>});</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function stripExtraColumns(obj) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const copy = {};</p>
<p class="p1"><span class="Apple-converted-space">      </span>for (const k in obj) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>if (k === "Year" || k === "Ref." || k.trim() === "") continue;</p>
<p class="p1"><span class="Apple-converted-space">        </span>copy[k] = obj[k];</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>return copy;</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function toCityRecord(obj) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const rec = {</p>
<p class="p1"><span class="Apple-converted-space">        </span>Country: (obj.Country || "").trim(),</p>
<p class="p1"><span class="Apple-converted-space">        </span>City: (obj.City || "").trim(),</p>
<p class="p1"><span class="Apple-converted-space">        </span>temps: MONTHS.map(m =&gt; parseTemp(obj[m]))</p>
<p class="p1"><span class="Apple-converted-space">      </span>};</p>
<p class="p1"><span class="Apple-converted-space">      </span>rec.invalid = rec.temps.some(x =&gt; Number.isNaN(x)) || !rec.Country || !rec.City;</p>
<p class="p1"><span class="Apple-converted-space">      </span>return rec;</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function sortedProfile(temps) { return temps.slice().sort((a,b) =&gt; b - a); }</p>
<p class="p1"><span class="Apple-converted-space">    </span>function calendarProfile(temps) { return temps; }</p>
<p class="p1"><span class="Apple-converted-space">    </span>function distanceSq(a,b){ let s=0; for(let i=0;i&lt;a.length;i++){ const d=a[i]-b[i]; s+=d*d; } return s; }</p>
<p class="p1"><span class="Apple-converted-space">    </span>function keyFor(r){ return `${r.Country} — ${r.City}`; }</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function buildSelectOptions(selectEl, records, selectedKey) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const sorted = records.slice().sort((x,y) =&gt;</p>
<p class="p1"><span class="Apple-converted-space">        </span>(x.Country.localeCompare(y.Country) || x.City.localeCompare(y.City))</p>
<p class="p1"><span class="Apple-converted-space">      </span>);</p>
<p class="p1"><span class="Apple-converted-space">      </span>selectEl.innerHTML = "";</p>
<p class="p1"><span class="Apple-converted-space">      </span>for (const r of sorted) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const opt = document.createElement("option");</p>
<p class="p1"><span class="Apple-converted-space">        </span>opt.value = keyFor(r);</p>
<p class="p1"><span class="Apple-converted-space">        </span>opt.textContent = opt.value;</p>
<p class="p1"><span class="Apple-converted-space">        </span>selectEl.appendChild(opt);</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (selectedKey) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const has = [...selectEl.options].some(o =&gt; o.value === selectedKey);</p>
<p class="p1"><span class="Apple-converted-space">        </span>selectEl.value = has ? selectedKey : selectEl.options[0]?.value ?? "";</p>
<p class="p1"><span class="Apple-converted-space">      </span>} else {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const addis = [...selectEl.options].find(o =&gt; /Addis Ababa$/i.test(o.textContent));</p>
<p class="p1"><span class="Apple-converted-space">        </span>selectEl.value = addis ? addis.value : selectEl.options[0]?.value ?? "";</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function getMethod() {</p>
<p class="p1"><span class="Apple-converted-space">      </span>return document.querySelector('input[name="method"]:checked')?.value || "sorted";</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p1"><span class="Apple-converted-space">    </span>function profileByMethod(temps, method) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>return method === "calendar" ? calendarProfile(temps) : sortedProfile(temps);</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p1"><span class="Apple-converted-space">    </span>function updateURLParams(selectedKey, method) {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const url = new URL(window.location.href);</p>
<p class="p1"><span class="Apple-converted-space">      </span>url.searchParams.set("city", selectedKey);</p>
<p class="p1"><span class="Apple-converted-space">      </span>url.searchParams.set("method", method);</p>
<p class="p1"><span class="Apple-converted-space">      </span>history.replaceState(null, "", url.toString());</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p1"><span class="Apple-converted-space">    </span>function readURLParams() {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const url = new URL(window.location.href);</p>
<p class="p1"><span class="Apple-converted-space">      </span>return { city: url.searchParams.get("city"), method: url.searchParams.get("method") || "sorted" };</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>const MONTHS_ARR = MONTHS;</p>
<p class="p1"><span class="Apple-converted-space">    </span>const targetSel = document.getElementById("targetSel");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const resultsTable = document.getElementById("resultsTable");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const resultsBody = document.getElementById("resultsBody");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const statusEl = document.getElementById("status");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const filterBox = document.getElementById("filterBox");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const emptyMsg = document.getElementById("emptyMsg");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const copyLinkBtn = document.getElementById("copyLinkBtn");</p>
<p class="p1"><span class="Apple-converted-space">    </span>const copyMsg = document.getElementById("copyMsg");</p>
<p class="p1"><span class="Apple-converted-space">    </span>let records = [], validRecords = [];</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function render() {</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (!validRecords.length) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>resultsTable.classList.add("hide");</p>
<p class="p1"><span class="Apple-converted-space">        </span>statusEl.innerHTML = '&lt;span class="badnews"&gt;No valid data found. Check the CSV inside the &amp;lt;script id="raw-data" type="text/plain"&amp;gt; tag.&lt;/span&gt;';</p>
<p class="p1"><span class="Apple-converted-space">        </span>return;</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>const selectedKey = targetSel.value;</p>
<p class="p1"><span class="Apple-converted-space">      </span>const method = getMethod();</p>
<p class="p1"><span class="Apple-converted-space">      </span>const target = validRecords.find(r =&gt; keyFor(r) === selectedKey) || validRecords[0];</p>
<p class="p1"><span class="Apple-converted-space">      </span>const targetProf = profileByMethod(target.temps, method);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>const rows = validRecords.map(r =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">        </span>const prof = profileByMethod(r.temps, method);</p>
<p class="p1"><span class="Apple-converted-space">        </span>return { ...r, dist: distanceSq(targetProf, prof) };</p>
<p class="p1"><span class="Apple-converted-space">      </span>}).sort((a,b) =&gt; a.dist - b.dist);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>const q = filterBox.value.trim().toLowerCase();</p>
<p class="p1"><span class="Apple-converted-space">      </span>const filtered = q ? rows.filter(r =&gt; (r.Country + " " + r.City).toLowerCase().includes(q)) : rows;</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>resultsBody.innerHTML = "";</p>
<p class="p1"><span class="Apple-converted-space">      </span>let rank = 0;</p>
<p class="p1"><span class="Apple-converted-space">      </span>for (const r of filtered) {</p>
<p class="p1"><span class="Apple-converted-space">        </span>rank++;</p>
<p class="p1"><span class="Apple-converted-space">        </span>const tr = document.createElement("tr");</p>
<p class="p1"><span class="Apple-converted-space">        </span>const tdRank = document.createElement("td"); tdRank.textContent = String(rank);</p>
<p class="p1"><span class="Apple-converted-space">        </span>const tdCountry = document.createElement("td"); tdCountry.textContent = r.Country;</p>
<p class="p1"><span class="Apple-converted-space">        </span>const tdCity = document.createElement("td"); tdCity.textContent = r.City;</p>
<p class="p1"><span class="Apple-converted-space">        </span>const tdDist = document.createElement("td"); tdDist.textContent = r.dist.toFixed(3);</p>
<p class="p1"><span class="Apple-converted-space">        </span>if (keyFor(r) === keyFor(target)) tdCountry.style.fontWeight = tdCity.style.fontWeight = tdDist.style.fontWeight = "700";</p>
<p class="p1"><span class="Apple-converted-space">        </span>tr.append(tdRank, tdCountry, tdCity, tdDist);</p>
<p class="p1"><span class="Apple-converted-space">        </span>resultsBody.appendChild(tr);</p>
<p class="p1"><span class="Apple-converted-space">      </span>}</p>
<p class="p1"><span class="Apple-converted-space">      </span>emptyMsg.classList.toggle("hide", filtered.length &gt; 0);</p>
<p class="p1"><span class="Apple-converted-space">      </span>resultsTable.classList.toggle("hide", filtered.length === 0);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>statusEl.innerHTML =</p>
<p class="p1"><span class="Apple-converted-space">        </span>`Loaded &lt;strong&gt;${validRecords.length}&lt;/strong&gt; cities. ` +</p>
<p class="p1"><span class="Apple-converted-space">        </span>`Comparing by &lt;span class="mono"&gt;${method === "sorted" ? "sorted distribution" : "calendar order"}&lt;/span&gt;.`;</p>
<p class="p1"><span class="Apple-converted-space">      </span>updateURLParams(selectedKey, method);</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>function init() {</p>
<p class="p1"><span class="Apple-converted-space">      </span>const raw = document.getElementById("raw-data").textContent.trim();</p>
<p class="p1"><span class="Apple-converted-space">      </span>console.log("Raw CSV length:", raw.length);</p>
<p class="p1"><span class="Apple-converted-space">      </span>const objs = textToObjects(raw).map(stripExtraColumns);</p>
<p class="p1"><span class="Apple-converted-space">      </span>console.log("Parsed rows:", objs.length, objs[0]);</p>
<p class="p1"><span class="Apple-converted-space">      </span>records = objs.map(toCityRecord);</p>
<p class="p1"><span class="Apple-converted-space">      </span>validRecords = records.filter(r =&gt; !r.invalid);</p>
<p class="p1"><span class="Apple-converted-space">      </span>console.log("Valid records:", validRecords.length, validRecords);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>const { city: urlCity, method: urlMethod } = readURLParams();</p>
<p class="p1"><span class="Apple-converted-space">      </span>buildSelectOptions(targetSel, validRecords, urlCity);</p>
<p class="p1"><span class="Apple-converted-space">      </span>if (urlMethod === "calendar") document.getElementById("methodCalendar").checked = true;</p>
<p class="p1"><span class="Apple-converted-space">      </span>else document.getElementById("methodSorted").checked = true;</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">      </span>render();</p>
<p class="p1"><span class="Apple-converted-space">    </span>}</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>targetSel.addEventListener("change", render);</p>
<p class="p1"><span class="Apple-converted-space">    </span>document.getElementById("methodSorted").addEventListener("change", render);</p>
<p class="p1"><span class="Apple-converted-space">    </span>document.getElementById("methodCalendar").addEventListener("change", render);</p>
<p class="p1"><span class="Apple-converted-space">    </span>filterBox.addEventListener("input", render);</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>copyLinkBtn.addEventListener("click", async () =&gt; {</p>
<p class="p1"><span class="Apple-converted-space">      </span>try { await navigator.clipboard.writeText(window.location.href);</p>
<p class="p1"><span class="Apple-converted-space">        </span>copyMsg.textContent = "Link copied!"; setTimeout(() =&gt; copyMsg.textContent = "", 1500);</p>
<p class="p1"><span class="Apple-converted-space">      </span>} catch { copyMsg.textContent = "Couldn’t copy (clipboard blocked)"; setTimeout(() =&gt; copyMsg.textContent = "", 2000); }</p>
<p class="p1"><span class="Apple-converted-space">    </span>});</p>
<p class="p2"><br></p>
<p class="p1"><span class="Apple-converted-space">    </span>init();</p>
<p class="p1"><span class="Apple-converted-space">  </span>&lt;/script&gt;</p>
<p class="p1">&lt;/body&gt;</p>
<p class="p1">&lt;/html&gt;</p>
</body>
</html>
